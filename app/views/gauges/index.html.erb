<h2>Gauges#index</h2>
<table>
  <tr>
    <th>メンバ</th>
    <% get_this_week().each do |date| %>
      <th><%= date.strftime("%m/%d") %></th>
    <% end %>
  </tr>
  <% @members.each do |member| %>
  <tr>
    <td><%= member.lastname %></td>
    <% get_this_week().each do |date| %>
      <td><%= date.strftime("%m/%d") %></td>
    <% end %>
  </tr>
  <% end %>
</table>
